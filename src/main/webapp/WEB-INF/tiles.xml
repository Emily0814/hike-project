<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>


	<!-- 그 외 페이지 -->
	<definition name="*.*" template="/WEB-INF/views/layout/index.jsp">
		<put-attribute name="main-menu" value="/WEB-INF/views/inc/main-menu.jsp"></put-attribute>
		<put-attribute name="asset" value="/WEB-INF/views/inc/asset.jsp"></put-attribute>
		<put-attribute name="content"
		value="/WEB-INF/views/{1}/{2}.jsp"></put-attribute>
		<put-attribute name="main-footer" value="/WEB-INF/views/inc/main-footer.jsp"></put-attribute>
	</definition>

	<!-- 홈페이지 -->
	<definition name="*" template="/WEB-INF/views/layout/index.jsp">
		<put-attribute name="main-menu" value="/WEB-INF/views/inc/main-menu.jsp"></put-attribute>
		<put-attribute name="asset" value="/WEB-INF/views/inc/asset.jsp"></put-attribute>
		<put-attribute name="content"
					   value="/WEB-INF/views/{1}.jsp"></put-attribute>
		<put-attribute name="main-footer" value="/WEB-INF/views/inc/main-footer.jsp"></put-attribute>
	</definition>



</tiles-definitions>












